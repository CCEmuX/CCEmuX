<?xml version="1.0" encoding="UTF-8"?>
<Configuration packages="net.clgd.ccemux">
	<Appenders>
		<Console name="CONSOLE">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} [%-5level] %msg%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="TRACE">
			<AppenderRef ref="CONSOLE" />
		</Root>
	</Loggers>
</Configuration>